[global_config]
  title_transmit_fg_color = "#2e3440"
  title_transmit_bg_color = "#2777ff"
  title_receive_fg_color = "#2E3440"
  title_receive_bg_color = "#8FBCBB"
  title_inactive_fg_color = "#D8DEE9"
  title_inactive_bg_color = "#4C566A"
[keybindings]
  split_horiz = <Primary>Down
  split_vert = <Primary>Right
[profiles]
  [[default]]
    background_darkness = 0.9
    background_type = transparent
    cursor_color = "#aaaaaa"
    font = MesloLGS NF 10
    foreground_color = "#00ff00"
    show_titlebar = False
    scrollback_lines = 10000
    palette = "#2e3436:#cc0000:#4e9a06:#c4a000:#3465a4:#75597b:#06989a:#d3d7cf:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#ad7fa8:#34e3e3:#eeeeec"
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
[plugins]
