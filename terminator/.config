[global_config]
  always_split_with_profile = True
[keybindings]
  split_horiz = <Primary>Down
  split_vert = <Primary>Right
[profiles]
  [[default]]
    background_color = "#141d2b"
    background_darkness = 0.9
    background_type = transparent
    font = MesloLGS NF 10
    foreground_color = "#a4b1cd"
    show_titlebar = False
    scrollback_lines = 10000
    palette = "#2e3436:#ff8484:#9fef00:#ffcc5c:#5cb2ff:#cf8dfb:#5cecc6:#d3d7cf:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#ad7fa8:#34e3e3:#eeeeec"
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
[plugins]
